<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <TextView
        android:id="@+id/txtMainTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"       
        android:text="@string/main_title_text" />
    


 
    <Button
        android:id="@+id/btnNextSub1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/txtMainTitle"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="100dp"
        android:text="@string/sub_title_text"
        android:visibility="visible"/>


    <Button
        android:id="@+id/btnNextSub3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/btnNextSub1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="Mobon Json Ad data Sample"
        android:visibility="visible"/>

    <Button
        android:id="@+id/btnNextSub4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/btnNextSub3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="@string/sub_title_text4" />

    <Button
        android:id="@+id/btnNextSub5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/btnNextSub4"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="@string/sub_title_text5" />

